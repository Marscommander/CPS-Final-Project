Cell["Formulas for Rocket", "Section",
 CellChangeTimes->{{3.6689001078335443`*^9, 3.668900113001707*^9}}],

Cell[TextData[{
 "Acceleration due to gravity at Earth\[CloseCurlyQuote]s Surface in ",
 Cell[BoxData[
  FormBox[
   RowBox[{"m", "/", 
    SuperscriptBox["s", "2"]}], TraditionalForm]],
  FormatType->"TraditionalForm"]
}], "Text",
 CellChangeTimes->{{3.6689004946192183`*^9, 3.6689005155294*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"g0", "=", "9.8"}], ";"}]], "Input",
 CellChangeTimes->{{3.6689004222225065`*^9, 3.6689004235780487`*^9}, {
  3.668900481053506*^9, 3.6689004886980343`*^9}}],

Cell["\<\
Specific Impuse of the engine in s. Vg is the velocity of the exhaust gas of \
the engine\
\>", "Text",
 CellChangeTimes->{{3.6689005296182165`*^9, 3.6689005758514614`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"Isp", "[", "Vg_", "]"}], "=", 
   RowBox[{"Vg", "/", "g0"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.6689002401316996`*^9, 3.6689003545824614`*^9}, {
  3.668900394197301*^9, 3.668900416672288*^9}}],

Cell["\<\
Delta V of the engine in m/s. m0 is initial mass, mt is mass at current time, \
Vg is exhaust gas of the engine, \[Rho]0 is the atmospheric pressure at \
altitude, R is the radius of the planet, h is the current altitude, H is the \
scaling factor for the atmosphere in m, g0 is the acceleration due to gravity \
for Earth, DeltaT is the change in time, and v is the instantaneous velocity \
of the rocket.\
\>", "Text",
 CellChangeTimes->{{3.668902041892913*^9, 3.668902107489472*^9}, {
  3.668902138493572*^9, 3.6689021703367558`*^9}, {3.66890248200892*^9, 
  3.668902502096324*^9}, {3.668902609695324*^9, 3.668902623781324*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"DeltaV", "[", 
    RowBox[{
    "Vg_", ",", "m0_", ",", "mt_", ",", "R_", ",", "h_", ",", "H_", ",", 
     "\[Rho]0_", ",", "v_", ",", "DeltaT_"}], "]"}], "=", 
   RowBox[{
    FractionBox[
     RowBox[{
      RowBox[{"Vg", "*", "m0"}], "-", 
      RowBox[{"m0", "*", "g0", "*", 
       SuperscriptBox[
        RowBox[{"(", 
         FractionBox["R", 
          RowBox[{"R", "+", "h"}]], ")"}], "2"]}], "-", 
      RowBox[{
       SuperscriptBox["\[Rho]0", 
        FractionBox[
         RowBox[{"-", "h"}], "H"]], "*", 
       SuperscriptBox["v", "2"]}]}], 
     RowBox[{"m0", "-", "mt"}]], "*", "DeltaT"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.668901093266453*^9, 3.668901315137638*^9}, {
  3.6689013685409775`*^9, 3.6689013797701006`*^9}, {3.668901952583983*^9, 
  3.6689019834690714`*^9}, {3.6689021187786007`*^9, 3.668902126250348*^9}, {
  3.6689024607727966`*^9, 3.668902467118431*^9}}],

